{% extends 'base.twig' %}

{% block content %}
    <form action="/login" method="post">
        <label>
            Username:<br/>
            <input type="text" name="username"/>
        </label>
        <br/>
        <label>
            Password:<br/>
            <input type="password" name="password"/>
        </label>
        <br/>
        <input type="submit" value="submit"/>
    </form>
{% endblock %}